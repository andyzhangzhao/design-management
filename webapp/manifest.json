{
  "_version": "1.32.0",
  "sap.app": {
    "id": "projectmanagement",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "dataSources": {
      "projectList": {
        "uri": "/sap/opu/odata/sap/ZRRE_C_DMCP_CDS/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "projectDetailsItems": {
        "uri": "/sap/opu/odata/sap/ZRRE_C_DCMSS_CDS/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "projectDetails": {
        "uri": "/sap/opu/odata/sap/ZRRE_C_DMHD_CDS/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "fullWidth": true,
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.100.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "projectmanagement.i18n.i18n"
        }
      },
      "": {
        "dataSource": "projectList",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupId": "$direct",
          "useBatch": false,
          "metadataUrlParams": {
            "sap-language": "ZH"
          }
        }
      },
      "detailsItems": {
        "dataSource": "projectDetailsItems",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupId": "$direct",
          "useBatch": false,
          "metadataUrlParams": {
            "sap-language": "ZH"
          }
        }
      },
      "details": {
        "dataSource": "projectDetails",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "earlyRequests": true,
          "defaultBindingMode": "sap.ui.model.BindingMode.TwoWay",
          "groupId": "$auto",
          "useBatch": true,
          "metadataUrlParams": {
            "sap-language": "ZH"
          }
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "projectmanagement.view",
        "controlAggregation": "pages",
        "controlId": "rootControl",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "projectList",
          "pattern": ":*all:",
          "target": ["projectList"]
        },
        {
          "name": "projectDetails",
          "pattern": "projectDetails/{devProjectID}/{designProjectID}/{section}/:filter:",
          "target": ["projectDetails"]
        },
        {
          "name": "designResultDetails",
          "pattern": "designResult/{devProjectID}/{designProjectID}/{mode}/:designResultID:",
          "target": ["designResultDetails"]
        }
      ],
      "targets": {
        "projectList": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewName": "ProjectList"
        },
        "projectDetails": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewName": "ProjectDetails"
        },
        "designResultDetails": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewName": "DesignResultManagementDetails"
        }
      }
    },
    "rootView": {
      "viewName": "projectmanagement.view.App",
      "type": "XML",
      "async": true
    }
  }
}
